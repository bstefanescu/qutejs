{
  "name": "qute",
  "version": "1.0.0-rc.9",
  "private": true,
  "scripts": {
    "build": "ws build",
    "dist": "ws dist",
    "test": "ws test",
    "start": "ws build && ws doc run 'watch doc' 'start doc'"
  },
  "author": "Bogdan Stefanescu <bogdan@quandora.com>",
  "license": "MIT",
  "description": "Qute monorepo",
  "homepage": "https://qutejs.org",
  "repository": {
    "url": "https://github.com/bstefanescu/qutejs",
    "type": "git"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@qutejs/register": "^1.0.0-rc.9",
    "@qutejs/rollup-plugin-qute": "^1.0.0-rc.9",
    "@qutejs/test-utils": "^1.0.0-rc.9",
    "cssnano": "^4.1.10",
    "mocha": "^6.2.3",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "@rollup/plugin-buble": "^0.21.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "build": {
    "scope": "@qutejs",
    "scripts": "build/scripts",
    "sources": "src/**",
    "test": {
      "files": [
        "test/**/*.test.js",
        "test/**/*.test.jsq"
      ],
      "setup": "./build/test/setup.js"
    },
    "configs": {
      "runtime": "build/config/runtime.config.js",
      "lib": "build/config/lib.config.js",
      "weblib": "build/config/weblib.config.js",
      "component": "build/config/component.config.js",
      "web": "build/config/web.config.js",
      "dev": "build/config/dev.config.js"
    },
    "projects": [
      "core/*",
      "tools/*",
      "plugins/*",
      "components/*",
      "web/polyfill",
      "web/dev",
      "web/doc"
    ],
    "devServer": [
      {
        "name": "doc",
        "root": "web",
        "index": "doc/index.html",
        "livereload": true,
        "open": true,
        "verbose": false,
        "host": "localhost",
        "https": {
          "key": "./build/localhost.key",
          "cert": "./build/localhost.crt"
        }
      }
    ],
    "tasks": {
      "dist": [
        "build prod ie",
        "test"
      ],
      "deploy": [
        "dist",
        "publish"
      ],
      "clean": [
        "unlink",
        "rm dist/* lib/*"
      ]
    }
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.3.2"
  }
}
