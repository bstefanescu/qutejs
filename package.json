{
  "name": "qute",
  "version": "2.0.0-beta.8",
  "private": true,
  "scripts": {
    "build": "ws build",
    "dist": "ws dist",
    "test": "ws test",
    "start": "ws build && ws doc run 'watch doc' 'start doc'"
  },
  "author": "Bogdan Stefanescu <bogdan@quandora.com>",
  "license": "MIT",
  "description": "Qute monorepo",
  "homepage": "https://qutejs.org",
  "repository": {
    "url": "https://github.com/bstefanescu/qutejs",
    "type": "git"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@qutejs/register": "2.0.0-beta.8",
    "@qutejs/rollup-plugin-qute": "2.0.0-beta.8",
    "@qutejs/test-utils": "2.0.0-beta.8",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "mocha": "^8.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "build": {
    "scope": "@qutejs",
    "scripts": "build/scripts",
    "sources": "src/**",
    "test": {
      "files": [
        "test/**/*.test.js",
        "test/**/*.test.jsq"
      ],
      "setup": "./build/test/setup.js"
    },
    "configs": {
      "runtime": "build/config/runtime.config.js",
      "lib": "build/config/lib.config.js",
      "weblib": "build/config/weblib.config.js",
      "component": "build/config/component.config.js",
      "web": "build/config/web.config.js",
      "dev": "build/config/dev.config.js"
    },
    "projects": [
      "core/*",
      "tools/*",
      "plugins/*",
      "components/*",
      "web/polyfill",
      "web/dev",
      "web/doc"
    ],
    "devServer": [
      {
        "name": "doc",
        "root": "web",
        "index": "doc/index.html",
        "livereload": true,
        "open": true,
        "verbose": false,
        "host": "localhost",
        "https": {
          "key": "./build/localhost.key",
          "cert": "./build/localhost.crt"
        }
      }
    ],
    "tasks": {
      "dist": [
        "build prod ie",
        "test"
      ],
      "deploy": [
        "dist",
        "publish"
      ],
      "clean": [
        "unlink",
        "shell rm -rf dist"
      ]
    }
  }
}